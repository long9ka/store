<table class="table table-striped custab mt-4">
    <thead>
        <tr>
            <th>ID</th>
            <th>username</th>
            <th>name</th>
            <th>email</th>
            <th>birthday</th>
            <th>gender</th>
            <th class="text-center">Action</th>
        </tr>
    </thead>
    <%users.forEach((e,i) =>{%>
    <tr>
        <td><%= i %></td>
        <td><%= e.username %></td>
        <td><%= e.profile.name %></td>
        <td><%= e.profile.email %></td>
        <td><%= moment(e.profile.birthday) %></td>
        <td><%= e.profile.gender %></td>
        <td class="text-center"><button type="button" class="btn btn-danger" onclick="del('<%= e.username %>')">Delete</button></td>
    </tr>
    <%});%>

</table>
